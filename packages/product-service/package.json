{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "jest -c ../../jest.config.js",
    "dev": "sls offline",
    "dynamodb:dev": "sls dynamodb start",
    "lint": "eslint --ignore-path .gitignore --fix src",
    "format": "npx prettier src --write",
    "deploy": "sls deploy -s dev --verbose",
    "deploy:dev": "sls deploy --stage dev --verbose",
    "deploy:prod": "sls deploy --stage prod --verbose",
    "doc:api": "sls openapi generate",
    "remove": "sls remove --verbose"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "ajv": "^8.11.0",
    "aws-sdk": "^2.1237.0",
    "axios": "^1.1.2",
    "dotenv": "^16.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@serverless/typescript": "^3.0.0",
    "@shelf/jest-dynamodb": "^3.3.1",
    "@swc/core": "^1.3.8",
    "@swc/jest": "^0.2.23",
    "@types/aws-lambda": "^8.10.71",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.1.2",
    "@types/node": "^14.14.25",
    "@types/yup": "^0.32.0",
    "esbuild": "^0.14.11",
    "fast-check": "^3.2.0",
    "jest": "^29.2.0",
    "json-schema-to-ts": "^1.5.0",
    "local-dynamo": "^0.6.0",
    "serverless": "^3.0.0",
    "serverless-auto-swagger": "^2.10.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^11.1.2",
    "serverless-openapi-documentation": "^0.4.0",
    "serverless-stage-manager": "^1.0.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
